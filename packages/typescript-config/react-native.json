{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "React Native",
    "extends": "./base.json",
    "compilerOptions": {
        "jsx": "react-native",
        "lib": [
            "ES2020"
        ],
        "module": "esnext",
        "target": "esnext",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true
    }
}