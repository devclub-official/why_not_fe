{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "check-types": {
      "cache": false
    },
    "ios": {
      "dependsOn": [
        "^ios"
      ]
    },
    "android": {
      "dependsOn": [
        "^android"
      ]
    }
  }
}